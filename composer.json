{
    "name": "ttbooking/model-extensions",
    "description": "Collection of useful Eloquent Model extensions",
    "type": "library",
    "keywords": ["eloquent", "model", "extensions", "traits", "concerns"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/ttbooking/model-extensions/issues",
        "source": "https://github.com/ttbooking/model-extensions"
    },
    "authors": [
        {
            "name": "Sergey Danilchenko",
            "email": "s.danilchenko@ttbooking.ru"
        }
    ],
    "require": {
        "php": "^7.1.3 || ^8.0",
        "illuminate/database": "^5.7.0 || ^6.20.26 || ^7.30.4 || ^8.40.0",
        "illuminate/support": "^5.7.0 || ^6.20.26 || ^7.30.4 || ^8.40.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0 || ^8.0 || ^9.0"
    },
    "suggest": {
	    "ramsey/uuid": "Required to use UUID primary keys (^3.7 || ^4.0)."
    },
    "autoload": {
        "psr-4": {
            "TTBooking\\ModelExtensions\\Concerns\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TTBooking\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
